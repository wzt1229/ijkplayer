      "GCC_PREPROCESSOR_DEFINITIONS": "$(inherited) IJK_USE_METAL_2=1",
      "METAL_LIBRARY_OUTPUT_DIR": "${CONFIGURATION_BUILD_DIR}/IJKMediaPlayerKit.framework/Resources",
      "MTL_LANGUAGE_REVISION": "Metal20"
    },
    "exclude_files": [
